{
  "name": "react-starter",
  "version": "0.9.0",
  "description": "A typescript, react, redux, sass, jest, webpack starter project",
  "main": "index.js",
  "scripts": {
    "start": "cross-env env=dev webpack serve --config webpack/webpack.config.ts",
    "build": "cross-env env=prod webpack --config webpack/webpack.config.ts",
    "analyze": "cross-env env=prod analyze=true webpack --config webpack/webpack.config.ts",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint-types": "tsc --noEmit",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "update": "yarn upgrade-interactive",
    "audit-npm": "yarn npm audit",
    "prepare": "husky install"
  },
  "author": "kaspars putniņš",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@statoscope/webpack-plugin": "^5.19.2",
    "@swc/core": "^1.2.124",
    "@swc/jest": "^0.2.15",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/css-minimizer-webpack-plugin": "^3.2.1",
    "@types/eslint": "^8.2.1",
    "@types/jest": "^27.4.0",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/node": "^17.0.6",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.21",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-dev-server": "^4.5.1",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "eslint": "^8.6.0",
    "eslint-config-claims-intake": "^3.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.5",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.1.4",
    "mini-css-extract-plugin": "^2.4.5",
    "prettier": "^2.5.1",
    "react-refresh": "^0.11.0",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.45.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.1.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest-junit": {
    "outputDirectory": "./coverage"
  },
  "packageManager": "yarn@3.1.1"
}
